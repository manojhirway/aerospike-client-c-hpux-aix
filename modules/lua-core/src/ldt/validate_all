# use this command to validate ALL of the Lua programs in the ldt space
# Usage:  validate_all

echo CRC32.lua
luac -p -l CRC32.lua | grep ETGLOBAL | grep -v "trace\|debug\|warn\|tostring\|map\|list\|aerospike\|info\|record\|error\|require\|bytes\|type"

echo ldt_common.lua
